# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'WeekendWarrior' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'CSV.swift', '~> 2.1.0'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
    pod 'AlgoliaSearch-Client-Swift', '~> 4.0'
    pod 'Firebase', '~> 4.1.1'
    pod 'Firebase/Core'
    pod 'FirebaseAnalytics', '~> 4.0.3'
    pod 'AFNetworking', '~> 3.0'
    pod 'Flurry-iOS-SDK/FlurrySDK' #Analytics Pod
    pod 'Flurry-iOS-SDK/FlurryAds' #Advertising Pod (requires Analytics)

  # Pods for WeekendWarrior

  target 'WeekendWarriorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WeekendWarriorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

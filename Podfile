    pod 'AWSAuthUI', '~> 2.6.6'

  # Pods for WeekendWarrior

  target 'WeekendWarriorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WeekendWarriorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
